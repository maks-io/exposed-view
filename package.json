{
  "name": "exposed-view",
  "version": "2.0.1",
  "description": "Simple react-native View wrapper, to expose its position / size via colors.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "keywords": [
    "react native",
    "view",
    "debug"
  ],
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "clear-build-folder": "rm -rf ./build",
    "transpile": "tsc",
    "fix-paths": "tsc-alias",
    "build": "yarn clear-build-folder && yarn transpile && yarn fix-paths",
    "dev": "yarn nodemon"
  },
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maks-io/exposed-view.git"
  },
  "author": "Markus Kurzmann <markus@kurzmann.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maks-io/exposed-view/issues"
  },
  "homepage": "https://github.com/maks-io/exposed-view#readme",
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@react-spring/native": "^9.7.3",
    "@types/node": "^20.11.25",
    "@types/react": "~18.2.64",
    "@types/react-native": "~0.72.8",
    "expo": "~50.0.11",
    "expo-status-bar": "~1.11.1",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.5",
    "react-native-reanimated": "~3.7.2",
    "react-native-web": "~0.19.10",
    "tsc": "^2.0.4",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "react-native-svg": "^15.1.0",
    "who-am-i-now": "^2.0.1"
  }
}
